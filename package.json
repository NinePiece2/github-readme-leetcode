{
  "name": "github-readme-leetocde",
  "version": "0.1.0",
  "private": true,
  "description": "Generate LeetCode stats SVG cards for your GitHub README",
  "author": "NinePiece2 <romit@romitsagu.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/NinePiece2/github-readme-leetocde"
  },
  "homepage": "https://github.com/NinePiece2/github-readme-leetocde",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
